{
  "name": "Forte.AI",
  "version": "1.0.0",
  "description": "Forte.AI - Hybrid GREKdev System with ML and OpenAI Agent",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
