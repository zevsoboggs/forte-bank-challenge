    "analyze:all": "ts-node --project tsconfig.node.json scripts/analyze-transactions.ts",
    "ml:train": "python ml-service/train_model.py",
    "ml:serve": "python ml-service/serve.py"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.14.0",
    "axios": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^12.23.24",
    "iconv-lite": "^0.7.0",
    "kafkajs": "^2.2.4",
    "lucide-react": "^0.378.0",
    "next": "^15.0.3",
    "next-auth": "^4.24.0",
    "next-swagger-doc": "^0.4.1",
    "nprogress": "^0.2.0",
    "openai": "^4.47.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
